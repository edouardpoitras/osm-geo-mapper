    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'osm_geo_mapper'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=osm-geo-mapper"
                ],
                "filter": {
                    "name": "osm-geo-mapper",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'osm-geo-mapper'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=osm-geo-mapper",
                    "--package=osm-geo-mapper"
                ],
                "filter": {
                    "name": "osm-geo-mapper",
                    "kind": "bin"
                }
            },
            "program": "${workspaceRoot}/target/debug/osm-geo-mapper",
            "args": ["--address", "ottawa ontario"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'osm-geo-mapper'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=osm-geo-mapper",
                    "--package=osm_geo_mapper"
                ],
                "filter": {
                    "name": "osm-geo-mapper",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}